<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:surfaceview="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    >

    <SurfaceView
        android:id="@+id/surfacePreview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />

    <SurfaceView
        android:id="@+id/surfaceOverlap"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        />

    <com.nhancv.facemask.m2d.M2DLandmarkView
        android:id="@+id/fragment_camera_2dlandmarkview"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:visibility="visible"
        />

    <org.rajawali3d.view.SurfaceView
        android:id="@+id/rajwali_surface"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        surfaceview:frameRate="30.0"
        surfaceview:renderMode="RENDER_WHEN_DIRTY"
        android:visibility="gone"
        />
</RelativeLayout>
